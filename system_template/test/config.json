{
	"name": "system_template example",
	"author": "Nusiq",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"profiles": {
			"dev": {
				"unsafe": false,
				"filters": [
					{
						"runWith": "python",
