{
	"name": "filter_tester example",
	"author": "Nusiq",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"profiles": {
			"dev": {
				"filters": [
					{
						"runWith": "python",
						"script": "../main.py",
						"settings": {
							"errors_stop_execution": true
						}
					}
				],
				"export": {
					"target": "development",
					"readOnly": true
				},
				"dataPath": "./data"
			}
		}
	}
}